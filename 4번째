#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;


int main()
{
	int n, m;
	int first, second, print;	

	while (true)
	{
		cin >> n;
		cin >> m;
		if (n < pow(10, 5) && m < pow(5, 3))
			break;
		else
			cout << "배열의 크기가 너무 크거나, 호출 횟수가 너무 많습니다!!\n";
	}
	int *a = new int[n];
	for (int i = 0; i < n; i++)
	{		
		int num = 0;
		cin >> num;
		while (true)
		{
			if (num > pow(10, 9))
				continue;
			break;
		}
		a[i] = num;
	}	
	for(int i = 0; i < m; i++)
	{
		cout << Q(first, second, print, a) + "\n";
	}
}

int Q(int i, int j, int k, int *a)
{	
	sort(a[i], a[j]);
	
	return a[k];
}
